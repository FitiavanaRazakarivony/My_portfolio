{
  "version": 2,
  "builds": [
    {
      "src": "dist/browser/projet/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/browser/projet/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
